{
    "name": "Python 3",
    "image": "mcr.microsoft.com/devcontainers/python:3",
    "features": {
      "ghcr.io/devcontainers/features/python:1": {
        "version": "3.9"
      }
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "customizations": {
